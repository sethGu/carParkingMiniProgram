.flex_center {
    display: -webkit-box;
    display: -webkit-flex;
    display: -ms-flexbox;
    display: flex;
    -webkit-box-align: center;
    -ms-flex-align: center;
    align-items: center;
}

.flex_full {
    -webkit-box-flex: 1;
    flex: 1;
}

.flex_half {
    -webkit-box-flex: 0.5;
    flex: 0.5;
}

.flex_half_2 {
    -webkit-box-flex: 0.25;
    flex: 0.25;
}

.flex_half_3 {
    -webkit-box-flex: 0.75;
    flex: 0.75;
}

.flex_twice {
    -webkit-box-flex: 1.5;
    flex: 1.5;
}

.container {
    height: 100%;
}

.custom_input_box {
    position: relative;
    /* margin: 10rpx 0; */
    line-height: 90rpx;
    font-size: 32rpx;
}

.custom_input_box .energy_icon {
    margin-right: 20rpx;
    width: 56rpx;
    height: 32rpx;
}

.custom_input_box .custom_placeholder {
    color: #888;
}

.custom_input_box .custom_lpn {
    color: #2c4358;
    letter-spacing: 4rpx;
    /* height: 100rpx; */
}

.cursor:after {
    content: "";
    width: 4rpx;
    height: 30rpx;
    background: url(data:image/gif;base64,) center left no-repeat;
    background-size: 4rpx 28rpx;
    padding-left: 30rpx;
}

.keyboard_box {
    position: fixed;
    bottom: 0;
    left: 0;
    right: 0;
    background-color: #e1e3e7;
    text-align: center;
    box-shadow: 0 -6rpx 22rpx #e9e9e9;
    z-index: 100;
    line-height: 1!important;
}

.keyboard_box .bar {
    height: 66rpx;
    background-color: #fff;
    position: relative;
}

.keyboard_box .bar .k_close {
    color: #259cf5;
    font-size: 32rpx;
    text-align: right;
    float: right;
    border: none;
    height: 100%;
    line-height: 66rpx;
}

.keyboard {
    text-align: center;
    padding: 13rpx 8rpx;
    max-width: 1000rpx;
    margin: 0 auto;
    position: relative;
}

.keyboard .key_box {
    margin-bottom: 10rpx;
}

.keyboard .key_box:last-child {
    margin-bottom: 0;
}

.keyboard .key_box .key view {
    padding-bottom: 154%;
    background-color: #fff;
    display: inline-block;
    width: 100%;
    margin: 0;
    border-radius: 10rpx;
    font-size: 40rpx;
    position: relative;
    box-shadow: 0 4rpx 4rpx #d7d7d7;
    z-index: 50;
}

.keyboard .key_box .key view text {
    position: absolute;
    top: 50%;
    height: 5.2rpx;
    line-height: 5.2rpx;
    width: 100%;
    left: 0;
    margin-top: -2rpx;
    font-style: normal;
    color: #212121;
}

.keyboard .key_box .key {
    margin-right: 10rpx;
    font-size: 0;
}

.keyboard .key_box .key:last-child {
    margin-right: 0;
}

.keyboard .key_box .key.empty {
    margin-right: 6rpx;
}

.keyboard .key_box .key.m_r_none {
    margin-right: 0;
}

.keyboard .key_box .key.empty view,.keyboard .key_box .key.empty_2 view,.keyboard .key_box .key.empty_3 view {
    margin: 0;
    background-color: rgba(0,0,0,0);
    box-shadow: none;
}

.keyboard .key_box .key.del view {
    padding-bottom: 104%;
    background: #c4c8cf url("data:image/png;base64,") no-repeat center center;
    background-size: 50%;
}

.keyboard .key_box .key.disable view {
    background-color: #e9edf1;
}

.keyboard .key_box .key view.float {
    z-index: 300;
}

.keyboard .click {
    position: absolute;
    top: -47rpx;
    left: 32rpx;
    z-index: -200;
    width: 60rpx;
    font-size: 0;
}

.keyboard .click .float_num {
    width: 100%;
    text-align: center;
    color: #212121;
    font-size: 56rpx;
    position: absolute;
    left: 0;
    top: 0;
    height: 60rpx;
    line-height: 60rpx;
}

.keyboard .click image {
    width: 100%;
    height: 100%;
}

.keyboard .click .cover {
    position: absolute;
    left: 0;
    top: 0;
    right: 0;
    bottom: 0;
    z-index: 10;
}

.clear-img {
    width: 40rpx;
    height: 40rpx;
    /* position: absolute;
    right: 20rpx;
    top: 30rpx; */
}